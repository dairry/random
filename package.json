    "eslint-plugin-jest-dom": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^1.5.0",
    "eslint-plugin-testing-library": "^3.1.3",
    "gh-pages": "^3.0.0",
